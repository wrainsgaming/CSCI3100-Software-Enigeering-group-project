<div class="jumbotron">
    <h1 class="display-3">{{ stock.companyName }} ({{ stock.symbol }}) </h1>
</div>


<div style="overflow:auto">
    <div class="menu">
        <a > <strong>Latest Price: ${{ stock.latestPrice }}</strong></a>
        <a > <strong>Previous Close: ${{  stock.previousClose  }}</strong></a>
        <a > <strong>Latest Price: ${{  stock.changePercent }}</strong></a>
        <a > <strong>Percent Change: ${{ stock.ytdChange  }}</strong></a>
        <a > <strong>52wk High: ${{ stock.week52High }}</strong></a>
        <a > <strong>52wk Low: ${{ stock.week52Low }}</strong></a>
    </div>

    <div class="main">
        <div class="StockRealTimeQuoteSystem">
            <br/><br/>
            <div id="StockRealTimeQuoteSystem"></div>
